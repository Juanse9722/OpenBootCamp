def bisiesto(año):
    divisiblePor4 = año % 4
    divisiblePor100 = año % 100
    divisiblePor400 = año % 400
    
    if divisiblePor4 == 0:
        if divisiblePor100 != 0 or divisiblePor400 == 0:
            print(f"El año {año} es bisiesto")
        else:
            print(f"El año {año} no es bisiesto")
    else:
        print(f"El año {año} no es bisiesto")
    
bisiesto(2023)
